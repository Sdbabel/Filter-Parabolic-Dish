          RowBox[{"1", ";;", 
           RowBox[{"Length", "[", "fileRawData", "]"}]}], "]"}], "]"}], ",", 
        "5"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"Clear", "[", "fileRawData", "]"}], ";", "\[IndentingNewLine]", 
     RowBox[{"fileValidPhoton", "=", 
