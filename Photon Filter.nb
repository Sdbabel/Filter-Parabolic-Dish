  RowBox[{"For", "[", 
   RowBox[{
    RowBox[{"i", "=", "1"}], ",", 
    RowBox[{"i", "\[LessEqual]", "numberOfFiles"}], ",", 
    RowBox[{"++", "i"}], ",", 
    RowBox[{
